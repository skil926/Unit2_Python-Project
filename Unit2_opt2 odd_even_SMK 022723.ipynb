   "source": [
    "#odd vs even\n",
    "\n",
    "print(\"Enter any number to test if it is odd or even\")\n",
    "\n",
    "num = int(input(\"Enter a number: \"))\n",
    "while num > 0:\n",
    "    if (num % 2) == 0:\n",
    "        print(\"{0} is Even\".format(num))\n",
    "    else:\n",
    "        print(\"{0} is Odd\".format(num))\n",
    "       \n",
    "    num = int(input(\"Enter a number: \"))\n"
   ]
